{
  "name": "@docfy/ember-cli",
  "version": "0.10.3",
  "description": "Ember CLI classic integration with Docfy",
  "keywords": [
    "ember-addon",
    "prember-plugin"
  ],
  "repository": "https://github.com/josemarluedke/docfy",
  "license": "MIT",
  "author": "Josemar Luedke <josemarluedke@gmail.com>",
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "scripts": {
    "prebuild": "npm-run-all compile",
    "build": "ember build --environment=production",
    "compile": "tsc --project src/tsconfig.json",
    "lint": "yarn lint:js && yarn lint:hbs",
    "lint:fix": "concurrently \"yarn:lint:*:fix\" --names \"fix:\" --prefixColors auto",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "format": "prettier . --cache --write",
    "format:check": "prettier . --cache --check",
    "prepare": "npm-run-all compile",
    "prestart": "npm-run-all compile",
    "start": "ember serve",
    "test": "npm-run-all lint:hbs test:*",
    "test:ember": "ember test --test-port=",
    "test:ember-compatibility": "echo 'ember try:each TODO'"
  },
  "dependencies": {
    "@docfy/core": "^0.10.0",
    "@docfy/ember": "^0.10.3",
    "broccoli-bridge": "^1.0.0",
    "broccoli-file-creator": "^2.1.1",
    "broccoli-funnel": "^3.0.8",
    "broccoli-merge-trees": "^4.2.0",
    "broccoli-node-api": "^1.7.0",
    "broccoli-plugin": "^4.0.7",
    "broccoli-source": "^3.0.1",
    "calculate-cache-key-for-tree": "2.0.0",
    "debug": "^4.4.1",
    "ember-auto-import": "^2.10.0",
    "ember-cli-babel": "^8.2.0",
    "ember-cli-htmlbars": "^6.3.0",
    "mdast-util-to-string": "^2.0.0",
    "remark-hbs": "^0.4.1",
    "unist-builder": "^2.0.3",
    "unist-util-find": "^1.0.2",
    "unist-util-visit": "^2.0.2"
  },
  "devDependencies": {
    "@babel/helper-define-map": "^7.18.6",
    "@docfy/plugin-with-prose": "^0.10.0",
    "@ember/optional-features": "^2.2.0",
    "@ember/test-helpers": "^5.2.2",
    "@glimmer/component": "^2.0.0",
    "@glimmer/tracking": "^1.1.2",
    "@mapbox/rehype-prism": "^0.5.0",
    "autoprefixer": "^10.4.21",
    "broccoli-asset-rev": "^3.0.0",
    "concurrently": "^9.1.2",
    "ember-auto-import": "^2.4.0",
    "ember-cli": "~6.5.0",
    "ember-cli-babel": "8.2.0",
    "ember-cli-dependency-checker": "^3.3.3",
    "ember-cli-fastboot": "^4.1.5",
    "ember-cli-fastboot-testing": "^0.6.2",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-uglify": "^3.0.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-load-initializers": "^3.0.1",
    "ember-maybe-import-regenerator": "^1.0.0",
    "ember-qunit": "^9.0.3",
    "ember-resolver": "^13.1.1",
    "ember-router-helpers": "^1.0.0",
    "ember-sinon": "^5.0.0",
    "ember-source": "~6.6.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-imports": "^4.3.0",
    "ember-template-lint": "^7.7.0",
    "ember-try": "^4.0.0",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-ember": "^12.7.0",
    "eslint-plugin-n": "^17.18.0",
    "globals": "^16.1.0",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "prember": "^1.1.0",
    "prettier": "^3.5.3",
    "qunit": "^2.24.1",
    "qunit-dom": "^2.0.0",
    "refractor": "^3.0.0",
    "remark-autolink-headings": "^6.0.0",
    "remark-code-import": "^0.2.0",
    "tailwindcss": "^2.0.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.38.0",
    "webpack": "^5.100.2"
  },
  "engines": {
    "node": ">= 12"
  },
  "publishConfig": {
    "access": "public"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "main": "ember-cli-addon.js",
    "before": [
      "prember"
    ]
  }
}
